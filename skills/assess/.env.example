# === CONNECTION (required -- set once, reused by all skills) ===
AIRD_CONNECTION_STRING=

# === CONTEXT (optional -- user context from interview skill) ===
AIRD_CONTEXT=

# === OUTPUT ===
# Format: stdout (JSON to stdout), markdown, json:<path>
AIRD_OUTPUT=json:report.json

# === THRESHOLDS (optional -- path to custom thresholds JSON) ===
AIRD_THRESHOLDS=

# === LOGGING ===
AIRD_LOG_LEVEL=info

# === INFRASTRUCTURE (optional -- override probe-based detection) ===
AIRD_OTEL_ENDPOINT=
